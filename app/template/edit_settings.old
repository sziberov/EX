<title>[title] - [title_edit_settings]</title>
<div _title="small">
	<a href="#view/[object_id]">[title]</a>
</div>
<div _table style="--columns: repeat(2, minmax(0, max-content));">
	<div>
		<div></div>
		<div _title centered_>[title_edit_settings]</div>
	</div>
	<div>
		<div>[string_search]</div>
		<div>
			<div _check>
				<div></div>
				<div>[string_hide_from_search]</div>
			</div>
		</div>
	</div>
	<div>
		<div>[string_alias]</div>
		<div>
			<input type="text">
			<button>[button_check]</button>
		</div>
	</div>
	<div>
		<div>[string_origin]</div>
		<div _flex="v stacked left">
			<div _check>
				<div></div>
				<div>[string_hide_default_referer]</div>
			</div>
			<div _check>
				<div></div>
				<div>[string_hide_author_and_times]</div>
			</div>
		</div>
	</div>
	<div>
		<div>[string_poster]</div>
		<div>
			<input type="text" placeholder="[string_file_number]">
		</div>
	</div>
	<div>
		<div>[string_navigation]</div>
		<div _flex="v left">
			<div _check>
				<div></div>
				<div>[string_display_search_bar]</div>
			</div>
			<div _grid="h">
				<select>
					<option selected>[string_as_cells]</option>
					<option>[string_as_list]</option>
				</select>
				<select>
					<option selected>[string_by_inclusion_time]</option>
					<option>[string_by_edit_time]</option>
					<option>[string_by_creation_time]</option>
					<option>[string_by_title]</option>
					<option>По популярности</option>
					<option>По посещяемости</option>
					<option>По обсуждаемости</option>
					<option>По рекомендуемости</option>
				</select>
				<select>
					<option>4</option>
					<option>6</option>
					<option>8</option>
					<option>12</option>
					<option>16</option>
					<option selected>24</option>
					<option>32</option>
					<option>48</option>
					<option>64</option>
					<option>96</option>
					<option>128</option>
					<option>192</option>
					<option>256</option>
				</select>
			</div>
		</div>
	</div>
	<div>
		<div>[string_files]</div>
		<div>
			<div _check>
				<div></div>
				<div>[string_hide_file_list]</div>
			</div>
		</div>
	</div>
	<div>
		<div>[string_restrictions]</div>
		<div _flex="v stacked left">
			<div _check>
				<div></div>
				<div>[string_deny_nonbookmark_inclusion]</div>
			</div>
			<div _check>
				<div></div>
				<div>[string_deny_claims]</div>
			</div>
		</div>
	</div>
	<div>
		<div></div>
		<div>
			<button onclick="Hash.set('edit/[object_id]')">[button_save]</button>
			<button onclick="Hash.set('edit/[object_id]')">[button_cancel]</button>
		</div>
	</div>
</div>
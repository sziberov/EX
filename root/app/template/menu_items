				<a href="#[url]">[title]</a>
			</module>
		</then>
		<else>
			<a href="#">[link_files]</a>
			<a href="#video">[link_video]</a>
			<a href="#audio">[link_audio]</a>
			<a href="#images">[link_images]</a>
			<a href="#texts">[link_texts]</a>
			<a href="#apps">[link_apps]</a>
			<a href="#games">[link_games]</a>
			<a href="#about">[link_about]</a>
			<a href="#search">[link_search]</a>
		</else>
	</condition>
</module>